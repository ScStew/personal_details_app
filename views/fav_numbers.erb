<h2><%=first%><%=last%> please enter your three favorite numbers</h2>
<form method="post" action="favnum">
	<input type="numbers" name="num1" required>
	<input type="numbers" name="num2" required>
	<input type="numbers" name="num3" required>
	<input type="submit" value="enter">
	<input type="hidden" name="f_name"
	value= "<%=first%>">
	<input type="hidden" name="l_name"
	value= "<%=last%>">
</form>